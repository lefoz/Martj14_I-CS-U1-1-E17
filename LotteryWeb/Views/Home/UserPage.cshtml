
@{
    ViewData["Title"] = "My Submissions";
}
<div>
    <div class="form">
        <div id="signup">
            <h2 align="center">Add Your Serial Number</h2>
            <form action="/" method="post">
                <div class="top-row">
                    <div class="field-wrap">
                        <label>
                            Email<span class="req">*</span>
                        </label>
                        <input type="email" required autocomplete="off" />
                    </div>
                    <div class="field-wrap">
                        <label>
                            Serial Number<span class="req">*</span>
                        </label>
                        <input type="text" required autocomplete="off" />
                    </div>
                </div>
                <button type="submit" class="button button-block" />Add serial To Submission</button>
            </form>
        </div>
    </div>
    </div>
